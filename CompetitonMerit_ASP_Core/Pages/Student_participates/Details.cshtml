@page
@model CompetitonMerit_ASP_Core.Pages.Student_participates.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<h1>Participant's Details</h1>

<div>
    <h2>Check the full Details of the student who participated in the Competition.</h2>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Student Name
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Student_participate.Student_Name)
        </dd>
        <dt class="col-sm-2">
            Student Age
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Student_participate.Student_age)
        </dd>
        <dt class="col-sm-2">
            Studnet Mobile No.
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Student_participate.Mobile_Number)
        </dd>
        <dt class="col-sm-2">
            Student's Address
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Student_participate.Student_Address)
        </dd>
        <dt class="col-sm-2">
            Student's Father Name
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Student_participate.Father_Name)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.Student_participate.Id">Edit</a> |
    <a asp-page="./Index">Back to List</a>
</div>
